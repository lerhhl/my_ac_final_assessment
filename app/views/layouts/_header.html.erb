<header class="clearfix">
  <nav id="banner", class="navbar navbar-light" style="background-color: #ad9669;">
    <div class="container">
      <div class="header-left">
        <%= link_to "AC Final Assessment", root_path, class: "navbar-brand" %>
      </div>
      <div class="header-right">
        <ul class="nav navbar-nav">
          <% if user_signed_in? == false %>
            <li><%= link_to 'Sign up', new_user_registration_path%></li>
            <li><%= link_to 'Sign in', new_user_session_path %></li>
          <% else %>
            <li><%= link_to 'Edit profile', edit_user_registration_path %></li>
            <li><%= link_to 'Log out', destroy_user_session_path, method: :delete %></li>
            <li><%= link_to 'New note', new_note_path %></li>
            <li><%= link_to 'All users', users_index_path %></li>
            <li><%= link_to 'All notes', notes_show_all_path %></li>
          <% end %>
        </ul>
      </div>
    </div>
  </nav>
</header>